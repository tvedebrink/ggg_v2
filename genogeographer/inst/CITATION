citHeader("To cite genogeographer in publications use:")

citEntry(
  entry = "Article",
  title        = "Weight of the evidence of genetic investigations of ancestry informative markers",
  author       = personList(as.person("Torben Tvedebrink"),as.person("Poul Svante Eriksen"),
                   as.person("Helle Smidt Mogensen"),as.person("Niels Morling")),
  journal      = "Theoretical Population Biology",
  year         = "2018",
  volume       = "120",
  pages        = "1--10",
  url          = "https://doi.org/10.1016/j.tpb.2017.12.004",

  textVersion  =
  paste("Torben Tvedebrink, Poul Svante Eriksen, Helle Smidt Mogensen, Niels Morling (2018).",
        "Weight of the evidence of genetic investigations of ancestry informative markers.",
        "Theoretical Population Biology, 120, 1-10.",
        "URL https://doi.org/10.1016/j.tpb.2017.12.004.")
)